<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.demo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".signup" android:screenOrientation="portrait"/>
        <activity android:name=".service" android:screenOrientation="portrait"/>
        <activity android:name=".fourbtn" android:screenOrientation="portrait"/>
        <activity android:name=".time" android:screenOrientation="portrait"/>
        <activity android:name=".gps" android:screenOrientation="portrait"/>
        <activity android:name=".set" android:screenOrientation="portrait"/>
        <activity android:name=".signup4" android:screenOrientation="portrait"/>
        <activity android:name=".signupFinish" android:screenOrientation="portrait"/>
        <activity android:name=".traintime" android:screenOrientation="portrait"/>
        <activity android:name=".service1" android:screenOrientation="portrait"/>
        <activity android:name=".servicefinish" android:screenOrientation="portrait"/>
        <activity android:name=".record" android:screenOrientation="portrait"/>
        <activity android:name=".setMember" android:screenOrientation="portrait"/>
        <activity android:name=".setPwd" android:screenOrientation="portrait"/>
        <activity android:name=".service_trainNo" android:screenOrientation="portrait"/>
        <activity android:name=".record_big" android:screenOrientation="portrait"/>
    </application>

</manifest>